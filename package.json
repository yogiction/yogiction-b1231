{
    "name": "starter-nextjs-theme",
    "version": "0.1.0",
    "engines": {
        "node": ">=14 <16",
        "npm": "~6"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build && next export",
        "start": "next start",
        "export": "next export",
        "contentful-typescript-codegen": "contentful-typescript-codegen --output src/types/contentful.d.ts && ./contentful-to-sourcebit-types.js",
        "validate": "stackbit validate",
        "analyze": "ANALYZE=true next build",
        "analyze:server": "BUNDLE_ANALYZE=server next build",
        "analyze:browser": "BUNDLE_ANALYZE=browser next build"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "classnames": "^2.3.1",
        "dayjs": "^1.10.7",
        "jose": "^3.14.3",
        "markdown-to-jsx": "^7.1.3",
        "next": "^11.1.2",
        "pure-react-carousel": "^1.28.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-responsive-carousel": "^3.2.23",
        "react-slick": "^0.28.1",
        "react-swipeable": "^6.2.1",
        "slick-carousel": "^1.8.1",
        "sourcebit": "^0.11.0",
        "sourcebit-source-contentful": "^0.6.7",
        "sourcebit-source-filesystem": "^0.2.0",
        "sourcebit-target-next": "^0.7.1",
        "tailwindcss": "^2.2.4"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^11.1.2",
        "@stackbit/cli": "^0.1.25",
        "@types/react": "^17.0.37",
        "@types/react-slick": "^0.23.8",
        "autoprefixer": "^10.2.6",
        "contentful-export": "^7.14.13",
        "contentful-import": "^8.2.28",
        "contentful-management": "^7.51.1",
        "contentful-typescript-codegen": "^3.2.3",
        "eslint": "^7.29.0",
        "eslint-config-next": "^11.0.1",
        "eslint-config-prettier": "^8.3.0",
        "postcss": "^8.3.5",
        "postcss-import": "^14.0.2",
        "postcss-nested": "^5.0.6",
        "prettier": "^2.3.2",
        "sass": "^1.40.0",
        "typescript": "^4.5.2",
        "webpack-bundle-analyzer": "^4.4.2"
    },
    "description": "A Nextjs page builder, component library and data source mapper all in one.",
    "main": "next.config.js",
    "license": "MIT"
